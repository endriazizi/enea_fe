{
  "$schema": "https://angular.dev/schema/ngsw-config.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": { "files": ["/index.html", "/*.css", "/*.js"] }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": { "files": ["/favicon.ico", "/assets/**", "/manifest.webmanifest"] }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["/api/**"],
      "cacheConfig": { "maxSize": 100, "maxAge": "3h", "timeout": "5s", "strategy": "performance" }
    }
  ]
}
